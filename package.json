{
  "name": "web3-simple-example",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:css:fix": "stylelint \"src/**/*.css\" --fix",
    "lint:docs": "markdownlint 'docs/**/*.md' --ignore node_modules",
    "lint:docs:fix": "markdownlint 'docs/**/*.md' --fix --ignore node_modules",
    "docs:links": "find docs -name '*.md' -not -path '*/node_modules/*' -exec markdown-link-check --config .markdown-link-check.json {} \\;",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:docs",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:docs:fix"
  },
  "dependencies": {
    "ethers": "^6.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@stylistic/eslint-plugin": "^2.11.0",
    "@stylistic/stylelint-config": "^3.0.1",
    "@stylistic/stylelint-plugin": "^4.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.15.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "markdown-link-check": "^3.14.1",
    "markdownlint-cli": "^0.45.0",
    "postcss": "^8.4.38",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-tailwindcss": "^0.0.7",
    "stylelint-order": "^6.0.4",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.3.1"
  }
}
